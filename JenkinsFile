pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                bat 'dotnet build'
            }
        }
        stage('Test') {
            steps {
                bat 'dotnet test'
            }
        }
        stage('Publish') {
    steps {
        bat 'dotnet publish SampleProject/SampleProject.csproj -c Release -o out'
    }
}

        }
    }
}
